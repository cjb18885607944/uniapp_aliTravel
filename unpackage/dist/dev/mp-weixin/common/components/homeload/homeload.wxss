
.homeload{
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 999;
	overflow: hidden;
	background-color: rgba(242,243,244,.6);
}
.loadimg{
	width: 100%;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.loadimg image{
	width: 200rpx;
	height: 200rpx;
	background-color: rgba(0,0,0,.2);
	border-radius: 10rpx;
}

